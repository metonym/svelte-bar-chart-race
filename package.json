{
  "name": "svelte-bar-chart-race",
  "version": "0.1.2",
  "license": "MIT",
  "description": "Bar chart race visualization for Svelte",
  "author": "Eric Liu (https://github.com/metonym)",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./*.svelte": {
      "types": "./dist/*.svelte.d.ts",
      "import": "./dist/*.svelte"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "svelte": "^3.51 || ^4"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/svelte": "^4.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vitest": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-bar-chart-race.git"
  },
  "homepage": "https://github.com/metonym/svelte-bar-chart-race",
  "bugs": "https://github.com/metonym/svelte-bar-chart-race/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "bar chart race"
  ],
  "files": [
    "dist"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ]
  }
}
