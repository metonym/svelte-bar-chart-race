{
  "name": "svelte-bar-chart-race",
  "version": "0.1.2",
  "license": "MIT",
  "description": "Bar chart race visualization for Svelte",
  "author": "Eric Liu (https://github.com/metonym)",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./*.svelte": {
      "types": "./dist/*.svelte.d.ts",
      "import": "./dist/*.svelte"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "peerDependencies": {
    "svelte": "^3.51 || ^4"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "3.0.0",
    "@sveltejs/kit": "^2.0.1",
    "@sveltejs/package": "^2.2.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.5",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-bar-chart-race.git"
  },
  "homepage": "https://github.com/metonym/svelte-bar-chart-race",
  "bugs": "https://github.com/metonym/svelte-bar-chart-race/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "bar chart race"
  ],
  "files": [
    "dist"
  ]
}
